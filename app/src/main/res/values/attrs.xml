<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="gauge">
        <attr name="PointSize" format="integer" />
        <attr name="PointStartColor" format="color" />
        <attr name="PointEndColor" format="color" />
        <attr name="StrokeColor" format="color" />
        <attr name="StrokeWidth" format="dimension" />
        <attr name="StrokeCap" format="string" />
        <attr name="StartAngle" format="integer" />
        <attr name="SweepAngle" format="integer" />
        <attr name="StartValue" format="integer" />
        <attr name="EndValue" format="integer" />
        <attr name="DividerSize" format="integer" />
        <attr name="DividerColor" format="color" />
        <attr name="DividerStep" format="integer" />
        <attr name="DividerDrawFirst" format="boolean" />
        <attr name="DividerDrawLast" format="boolean" />
    </declare-styleable>

    <bool name="isTablet">false</bool>

    <declare-styleable name="GaugesLayout">
        <attr name="layoutType" format="string" />
    </declare-styleable>
</resources>